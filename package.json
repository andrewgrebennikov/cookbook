{
  "name": "cookbook",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "start": "concurrently \"npm run start:server\" \"npm run start:dev\"",
    "start:dev": "rsbuild dev --open --port 3000",
    "start:server": "node json-server/index.js",
    "build:prod": "rsbuild build --env-mode .env.production",
    "build:dev": "rsbuild build --env-mode .env.development",
    "preview": "rsbuild preview",
    "prettier": "prettier --write ./**/*.{js,jsx,ts,tsx}",
    "lint:eslint": "eslint \"./**/*.{js,jsx,ts,tsx}\"",
    "lint:eslint:fix": "eslint \"./**/*.{js,jsx,ts,tsx}\" --fix",
    "lint:stylelint": "stylelint \"./**/*.scss\"",
    "lint:stylelint:fix": "stylelint \"./**/*.scss\" --fix"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "axios": "^1.7.0",
    "bootstrap": "^5.3.3",
    "classix": "^2.1.37",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.23.1"
  },
  "devDependencies": {
    "@conarti/eslint-plugin-feature-sliced": "^1.0.5",
    "@rsbuild/core": "^0.6.15",
    "@rsbuild/plugin-react": "^0.6.15",
    "@rsbuild/plugin-svgr": "^0.6.15",
    "@rsbuild/plugin-type-check": "^0.6.15",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-i18next": "^6.0.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "json-server": "^0.17.4",
    "stylelint": "^16.5.0",
    "stylelint-config-clean-order": "^5.4.2",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-prettier": "^5.0.0",
    "typescript": "^5.4.5"
  },
  "volta": {
    "node": "20.13.1"
  }
}
